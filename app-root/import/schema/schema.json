{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/Welcome6",
  "definitions": {
    "Welcome6": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "order_id": {
          "type": "string",
          "format": "integer"
        },
        "order_datetime": {
          "type": "string",
          "format": "date-time"
        },
        "customer": {
          "$ref": "#/definitions/Customer"
        },
        "items": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Item"
          }
        },
        "shipping_price": {
          "type": "number"
        },
        "discounts": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Discount"
          }
        }
      },
      "required": [
        "customer",
        "discounts",
        "items",
        "order_datetime",
        "order_id",
        "shipping_price"
      ],
      "title": "Welcome6"
    },
    "Customer": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "string",
          "format": "integer"
        },
        "first_name": {
          "type": "string"
        },
        "last_name": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "phone": {
          "type": "string"
        },
        "shipping_address": {
          "$ref": "#/definitions/ShippingAddress"
        }
      },
      "required": [
        "email",
        "first_name",
        "id",
        "last_name",
        "phone",
        "shipping_address"
      ],
      "title": "Customer"
    },
    "ShippingAddress": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "street": {
          "type": "string"
        },
        "postcode": {
          "type": "string",
          "format": "integer"
        },
        "suburb": {
          "type": "string"
        },
        "state": {
          "type": "string"
        }
      },
      "required": [
        "postcode",
        "state",
        "street",
        "suburb"
      ],
      "title": "ShippingAddress"
    },
    "Discount": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "type": {
          "type": "string"
        },
        "value": {
          "type": "integer"
        }
      },
      "required": [
        "type",
        "value"
      ],
      "title": "Discount"
    },
    "Item": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "quantity": {
          "type": "integer"
        },
        "unit_price": {
          "type": "number"
        },
        "product": {
          "$ref": "#/definitions/Product"
        }
      },
      "required": [
        "product",
        "quantity",
        "unit_price"
      ],
      "title": "Item"
    },
    "Product": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "product_id": {
          "type": "string",
          "format": "integer"
        },
        "title": {
          "type": "string"
        },
        "image": {
          "type": "string",
          "format": "uri",
          "qt-uri-protocols": [
            "http"
          ]
        },
        "upc": {
          "type": "string"
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "brand": {
          "$ref": "#/definitions/Brand"
        }
      },
      "required": [
        "brand",
        "created_at",
        "image",
        "product_id",
        "title",
        "upc"
      ],
      "title": "Product"
    },
    "Brand": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "brand_id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        }
      },
      "required": [
        "brand_id",
        "name"
      ],
      "title": "Brand"
    }
  }
}
